<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EasyTitleBar">
        <!--标题文字-->
        <attr name="title_text" format="string" />
        <!--标题大小-->
        <attr name="title_size" format="dimension" />
        <!--标题颜色-->
        <attr name="title_color" format="reference" />
        <!--标题位置-->
        <attr name="title_style" format="string" />
        <!--左边第一个图标的有无-->
        <attr name="left_image_state" format="string" />



        <attr name="titlebarRightText" format="string" />
        <attr name="titlebarLeftState" format="string" />
        <attr name="titlebarLeftImage" format="reference" />
        <attr name="titlebarRightImage" format="reference" />
        <attr name="titlebarBackground" format="reference|color" />
        <attr name="titlebarBackgroundColor" format="color" />
    </declare-styleable>
</resources>